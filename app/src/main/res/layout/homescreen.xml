<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#00000000"
    android:touchscreenBlocksFocus="false">

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="170dp"
        android:background="#96000000">

        <ImageButton
            android:layout_width="190dp"
            android:layout_height="match_parent"
            android:id="@+id/apartments"
            android:src="@mipmap/apartments"
            android:layout_gravity="center_vertical"
            android:background="#96000000"
            android:clickable="true" />

        <ImageButton
            android:layout_width="fill_parent"
            android:layout_height="match_parent"
            android:id="@+id/dining"
            android:src="@mipmap/dining"
            android:layout_gravity="center_vertical"
            android:background="#96000000"
            android:clickable="true" />
    </LinearLayout>

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="170dp"
        android:background="#96000000">

        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:id="@+id/education"
            android:src="@mipmap/education"
            android:background="#96000000"
            android:baselineAlignBottom="false"
            android:clickable="true" />

        <ImageButton
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/emergency"
            android:src="@mipmap/emergency"
            android:layout_gravity="center_horizontal"
            android:background="#96000000"
            android:clickable="true" />

    </LinearLayout>

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="170dp"
        android:background="#96000000">

        <ImageButton
            android:layout_width="190dp"
            android:layout_height="match_parent"
            android:id="@+id/entertainment"
            android:src="@mipmap/entertainment"
            android:layout_gravity="center_vertical"
            android:clickable="true"
            android:background="#96000000" />

        <ImageButton
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/tourism"
            android:src="@mipmap/tourism"
            android:adjustViewBounds="false"
            android:layout_gravity="center_vertical"
            android:clickable="true"
            android:background="#96000000" />
    </LinearLayout>

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#ca000000"></LinearLayout>

</LinearLayout>